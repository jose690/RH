        <meta name="description" content="Pinegrow Web Editor - SAAS TailwindCSS Template"> 
        <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>         
        <script src="~/js/pgia.js"></script>         
        <script src="~/js/Usuario.js"></script>         
        <script src="~/js/Oferente.js"></script>         
    </head>     
    <body class="font-light text-gray-700"> 
        <div class="flex h-16 justify-end"><a href="@Url.Action(" ndex")"><button type="button" class="bg-blue-600 font-medium mr-5 mt-5 px-6 py-2 rounded-md text-center text-white w-20 hover:bg-blue-700">Inicio</button></a> 
        </div>         
        <section class="bg-gray-50 pt-0 text-gray-500"> 
            <div class="container mx-auto px-4"> 
                <div class="mx-auto md:w-9/12 xl:w-5/12" style="display: flex;justify-content: center;"> 
                    <div class="bg-white px-6 py-12 w-full sm:px-12 lg:py-16" style="border: solid black;border-radius: 30px;width: 75%;"> 
                        <h2 class="flex font-medium justify-center mb-2 text-3xl text-gray-900">Registrarse</h2> 
                        <form> 
                            <div class="mb-4 text-sm"> 
                                <label for="tipoIdentificacion" class="block border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full" style="width: 55%;">Tipo de
                                    identificación
</label>                                 
                                <select id="tipoIdentificacion" name="tipoIdentificacion" class="block border border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full" style="width: 75%;"> 
                                    <option value="">Seleccione tipo de identificación</option>                                     
                                    <option value="Cédula de identificación">Cédula de identificación</option>                                     
                                    <option value="Dimex">Dimex</option>                                     
                                    <option value="Pasaporte">Pasaporte</option>                                     
                                </select>                                 
                            </div>                             
                            <div class="mb-4 text-sm" id="id" style="display: flex;"> 
                                <div style="display: flex;width: 60^;">
                                    <label for="cedula" class="block border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full" style="width: 35%;">Identificación</label>
                                    <input type="text" id="cedula" name="cedula" placeholder="01-0123-0123" class="block border border-gray-300 flex-1 mr-5 px-3 py-2 rounded-r-md w-full" aria-label="Identificación" required style="width: 70%;margin: 0;">
                                </div>                                 
                                <div class="flex" style="width: 50%; justify-content: flex-end;"> 
                                    <button type="button" id="buscarBtn" class="bg-blue-600 font-medium inline-block px-6 py-2 rounded-md text-center text-white w-3/12 hover:bg-blue-700" style="width: 75%;margin-left: 50%;">Buscar</button>                                     
                                </div>                                 
                            </div>                             
                            <!-- Campos para rellenar -->                             
                            <div class="flex"> 
                                <div class="mb-4 mr-5 text-sm w-6/12" style="width: 45%;"> 
                                    <label for="nombre" class="block border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full">Nombre</label>                                     
                                    <input type="text" id="nombre" name="nombre" class="block border border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full" aria-label="Nombre" required readonly> 
                                </div>                                 
                                <div class="mb-4 ml-5 text-sm w-6/12" style="width: 55%;"> 
                                    <label for="apellidos" class="block border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full">Apellidos</label>                                     
                                    <input type="text" id="apellidos" name="apellidos" class="block border border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full" aria-label="Apellidos" required readonly> 
                                </div>                                 
                            </div>                             
                            <div class="flex"> 
                                <div class="mb-4 mr-5 text-sm w-6/12"> 
                                    <label for="fechaNacimiento" class="block border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full">Fecha de
                                        nacimiento
</label>                                     
                                    <input type="text" id="fechaNacimiento" name="fechaNacimiento" class="block border border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full" aria-label="Fecha de nacimiento" required readonly style="width: 70%;"> 
                                </div>                                 
                                <div class="mb-4 ml-5 text-sm w-6/12"> 
                                    <label for="genero" class="block border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full">Género</label>                                     
                                    <input type="text" id="genero" name="genero" class="block border border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full" aria-label="Genero" required readonly style="width: 30%;"> 
                                </div>                                 
                            </div>                             
                            <div class="flex"> 
                                <div class="mb-4 mr-5 text-sm w-6/12" style="width: 40%;"> 
                                    <label for="telefono" class="block border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full">Teléfono
                                        celular
</label>                                     
                                    <input type="text" id="telefono" name="telefono" class="block border border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full" aria-label="Teléfono celular" required style="/* width: 50% */"> 
                                </div>                                 
                                <div class="mb-4 ml-5 text-sm w-6/12" style="justify-content: center;width: 60%;"> 
                                    <label for="correo" class="block border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full" style="text-align: center;">Correo
                                        electrónico
</label>                                     
                                    <input type="text" id="correo" name="correo" class="block border border-gray-300 flex-1 px-3 py-2 rounded-r-md w-full" aria-label="Correo electrónico" required> 
                                </div>                                 
                            </div>                             
                            <button type="button" id="registrarBtn" class="bg-blue-600 font-medium hover:bg-blue-700 inline-block px-6 py-2 rounded-md text-center text-white w-full">Registrarse</button>                             
                            <div class="flex items-center justify-center mb-4 mt-5"> 
                                <p>¿Ya tienes cuenta?</p><a href="@Url.Action(" ogin")" class="block ml-2 text-sm">Iniciar
                                sesión</a> 
                            </div>                             
                        </form>                         
                    </div>                     
                </div>                 
            </div>             
        </section>         
    </body>     
</html>